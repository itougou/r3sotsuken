<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.repository.StaffMapper">

    <select id="findByIdPass" resultType="com.example.demo.entity.Staff">
        SELECT * FROM Staff WHERE id = #{id} AND pass =  #{pass}
    </select>
    
</mapper>